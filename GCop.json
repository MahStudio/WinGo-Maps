{
  "$schema":"http://gcop.co/public/js/gcop-schema.json",
  "ReadMe": "To learn how to configure this, see:http://gcop.co/doc/config",
  "Projects": [],
  "DomainProjectNames": "",
  "General": [
    {
      "Path": "*.g.i.cs",
      "Exclude": "*"
    },
    {
      "Path": "*\\DAL\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\Pages\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\Entities\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\@Modules\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\Modules\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\Controller.cs",
      "Exclude": "*"
    },
    {
      "Path": "*\\Test\\*",
      "Exclude": "*"
    },
    {
      "Path": "*\\TaskManager.cs",
      "Exclude": "*"
    }
  ],
  "Report": {
    "Enabled": true,
    "AnalyzerErrorInterval": 30,
    "AnalyzerPerformanceInterval": 30,
    "ProjectInfoInterval": 30
  },
  "Logging": {
    "LogExceptionsToFile": false,
    "LogExceptionsToOutputWindow": false
  }
}