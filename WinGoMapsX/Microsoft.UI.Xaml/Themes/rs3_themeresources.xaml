                                            <Storyboard>
                                                <Storyboard.Children>
                                                    <FadeOutThemeAnimation Storyboard.TargetName="ZoomedInPresenter" />
                                                    <FadeInThemeAnimation Storyboard.TargetName="ZoomedOutPresenter" />
                                                </Storyboard.Children>
                                            </Storyboard>
